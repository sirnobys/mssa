<!DOCTYPE html>
<html lang="en">
<%-include partials/head.ejs-%>
<body class="hold-transition sidebar-mini layout-fixed layout-navbar-fixed sidebar-collapse">
<div class="wrapper">

  <!-- Navbar -->
  <%-include partials/navbar.ejs-%>
  <!-- /.navbar -->

  <!-- Main Sidebar Container -->
  <%-include partials/sidebar.ejs-%>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark">Issue Manager</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item active">Home</li>
              <li class="breadcrumb-item "><a href="/assigned">Assigned Issues</a></li>
              <li class="breadcrumb-item "><a href="/completed">Completed</a></li>
              <li class="breadcrumb-item "><a href="/issues">New Issues</a></li>
            </ol>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <%-include partials/ticker.ejs-%>
      </div>
    </section>
        <!-- /.row -->
        <!-- Main row -->
      <section class="content">
      <div class="container-fluid">
        <div class="">
          
            
            <!-- /.card -->

            <div class="card">
             <div class="card-header bg-dark">
                <h3 class="card-title ">All Issues</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <table id="example1" class="table table-bordered table-striped">
                  <thead class="bg-dark">
                  <tr>
                    <th>Request ID</th>
                    <th>Name</th>
                    <th>ID</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Issue</th>
                  </tr>
                  </thead>
                  <tbody>
                    <%  problem.map((problems) => { %>
                      <tr>
                        <td><%= problems.id%></td>
                        <td><%= problems.name %></td>                   
                        <td><%= problems.staff_student_id%></td>
                        <td><%= problems.email%></td>  
                        <td><%= problems.phone%></td>  
                        <td><%= problems.issue%></td>
                                                                                                            
                      
                      </tr>
                      <% }) %>
                    
  
                  </tbody>
                  <tfoot class="bg-dark">
                  <tr>
                    <th>Request ID</th>
                    <th>Name</th>
                    <th>ID</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Issue</th>
                  </tr>
                  </tfoot>
                </table>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- /.col -->
        </div>
        <!-- /.row -->
     
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
        <!-- /.row (main row) -->
      <!-- /.container-fluid -->
    
    <!-- /.content -->
 
  <!-- /.content-wrapper -->

  <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
  <!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->
<%-include partials/footer.ejs-%>


<!-- jQuery -->
<%-include partials/scripts.ejs-%>

</body>
</html>
